type User {
  id: ID! @unique
  name: String!
  email: String @unique
  password: String!
  order: Order
}

type Product {
  id: ID! @unique
  imageUrl: String!
  title: String!
  price: Float!
  orderedProducts: [OrderedProduct]!
}

type Order {
  id: ID! @unique
  totalPrice: Float! @default(value: 0)
  orderedProducts: [OrderedProduct!]!
  whoOrdered: User!
}

type OrderedProduct {
  id: ID! @unique
  product: Product!
  quantity: Int!
  order: Order!
}
